void inssert end(int item,NODE head)
{
	NODE p,q ;
	q=create() ;
	q-> data=item ;
	p=head ;
	while(p->next!=NULL)
	{
	 p=p->next ;
	}
	p->next=q ;
	q->next=NULL ;
}
