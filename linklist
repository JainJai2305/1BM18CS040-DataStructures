# include<stdio.h>
struct list 
{
	int data;
	struct*next ;
	typedef struct*list NODE ;
	NODE create() 
	{
		NODE p=(NODE)malloc(struct list)
		if(p==null)
		{
		 printf("memory not allocated") ;
		 return NULL ;
		}
		return p ;
	}
